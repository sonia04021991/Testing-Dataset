    <!-- http://hxl.humanitarianresponse.info/ns/#personCount -->

    <owl:DatatypeProperty rdf:about="http://hxl.humanitarianresponse.info/ns/#personCount">
        <rdfs:subPropertyOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#count"/>
        <rdfs:domain rdf:resource="http://hxl.humanitarianresponse.info/ns/#Population"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#popPlaceClassNumber -->

    <owl:DatatypeProperty rdf:about="http://hxl.humanitarianresponse.info/ns/#popPlaceClassNumber">
        <rdfs:domain rdf:resource="http://hxl.humanitarianresponse.info/ns/#PopPlaceClass"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#popPlaceClassTitle -->

    <owl:DatatypeProperty rdf:about="http://hxl.humanitarianresponse.info/ns/#popPlaceClassTitle">
        <rdfs:domain rdf:resource="http://hxl.humanitarianresponse.info/ns/#PopPlaceClass"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#title -->

    <owl:DatatypeProperty rdf:about="http://hxl.humanitarianresponse.info/ns/#title">
        <rdfs:domain rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#toAge -->

    <owl:DatatypeProperty rdf:about="http://hxl.humanitarianresponse.info/ns/#toAge">
        <rdfs:domain rdf:resource="http://hxl.humanitarianresponse.info/ns/#AgeGroup"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#validOn -->

    <owl:DatatypeProperty rdf:about="http://hxl.humanitarianresponse.info/ns/#validOn">
        <rdfs:subPropertyOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#date"/>
        <rdfs:domain rdf:resource="http://hxl.humanitarianresponse.info/ns/#DataContainer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://hxl.humanitarianresponse.info/ns/#APL -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#APL">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#LocatedBaseClass"/>
        <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
        <rdfs:comment xml:lang="en">A site providing shelter for an affected population (such as refugees). Examples include camps and temporary settlements. &lt;br /&gt;&lt;small&gt;This class has been added for demo purposes only, a proper definition will be provided in subsequent versions of the vocabulary.&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#geolocationSection"/>
        <skos:prefLabel xml:lang="en">Affected Population Location</skos:prefLabel>
        <label:plural xml:lang="en">Affected Population Locations</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#AdminUnit -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#AdminUnit">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#LocatedBaseClass"/>
        <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
        <rdfs:comment xml:lang="en">Administrative units can include coutries, regions, cities, etc. The hierarchy of administrative units can vary from country to country.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#geolocationSection"/>
        <skos:prefLabel xml:lang="en">Administrative Unit</skos:prefLabel>
        <label:plural xml:lang="en">Administrative Units</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#AdminUnitLevel -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#AdminUnitLevel">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <rdfs:comment xml:lang="en">The specific administrative unit levels can vary from country to country. Administrative Unit Level 0 is a country  in this case, hxl:Country should be used.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#geolocationSection"/>
        <skos:prefLabel xml:lang="en">Administrative Unit Level</skos:prefLabel>
        <label:plural xml:lang="en">Administrative Unit Levels</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#AffectedPopulation -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#AffectedPopulation">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#Population"/>
        <rdfs:comment xml:lang="en">A population affected by an emergency. &lt;br /&gt;&lt;small&gt;Term origin: Humanitarian Profile (&lt;em&gt;Affected&lt;/em&gt;);&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:prefLabel xml:lang="en">Affected Population</skos:prefLabel>
        <label:plural xml:lang="en">Affected Populations</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#AgeGroup -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#AgeGroup">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <rdfs:comment>Generic class for age groups; instances should be created for actual age groups and must have an identifier that does not start with a number. Example: &lt;em&gt;Children under 5&lt;/em&gt;. Each AgeGroup should be added to at least one &lt;a href=&apos;#AgeGroupSet&apos;&gt;AgeGroupSet&lt;/a&gt; following the conventions of &lt;a href=&apos;http://www.w3.org/TR/rdf-schema/#ch_member&apos;&gt;rdfs:member&lt;/a&gt;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:prefLabel xml:lang="en">Age Group</skos:prefLabel>
        <label:plural xml:lang="en">Age Groups</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#AgeGroupSet -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#AgeGroupSet">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Bag"/>
        <rdfs:comment xml:lang="en">Contains a set of bracketed age groups, usually defined as a standard by some authority. &lt;a href=&apos;#AgeGroup&apos;&gt;AgeGroups&lt;/a&gt; should be added to an AgeGroupSet following the conventions of &lt;a href=&apos;http://www.w3.org/TR/rdf-schema/#ch_member&apos;&gt;rdfs:member&lt;/a&gt;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:prefLabel xml:lang="en">Age Group Set</skos:prefLabel>
        <label:plural xml:lang="en">Age Group Sets</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#ApprovalLevel -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#ApprovalLevel">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <rdfs:comment>Instances of this class define fixed levels of approval in the HXL data repository.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#metadataSection"/>
        <skos:prefLabel xml:lang="en">Approval Level</skos:prefLabel>
        <label:plural xml:lang="en">Approval Levels</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#BaseClass -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#BaseClass">
        <rdfs:comment xml:lang="en">Abstract base class. Any other class in HXL is a subclass of hxl:BaseClass.</rdfs:comment>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#Casualty -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#Casualty">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#AffectedPopulation"/>
        <rdfs:comment>Examples: &lt;em&gt;Injured&lt;/em&gt; or &lt;em&gt;dead&lt;/em&gt;.&lt;br /&gt;&lt;small&gt;Term origin: ACAPS DSS, Humanitarian Profile.&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:prefLabel xml:lang="en">Casualties</skos:prefLabel>
        <label:plural xml:lang="en">Casualties</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#Country -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#Country">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#AdminUnit"/>
        <rdfs:comment xml:lang="en">Highest-level administrative unit (level 0). This includes but is not limited to the three-letter country codes defined by ISO 3166-1, which may represent nations, dependent territories, and special areas of geographical interest. Instantiation of a hxl:Country object is for the administrative purposes of the Humanitarian Exchange Language and does not imply endoresement or recognition of any type by anybody.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#geolocationSection"/>
        <skos:prefLabel xml:lang="en">Country</skos:prefLabel>
        <label:plural xml:lang="en">Countries</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#DataContainer -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#DataContainer">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <rdfs:comment>This is the &lt;em&gt;container&lt;/em&gt; element to attach any metadata to. Any HXL file has to include exactly one data container element and make all metadata statements about this container. When loading the file into the triple store, a new named graph will be created that inherits the metadata from this container.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#metadataSection"/>
        <skos:prefLabel xml:lang="en">Data Container</skos:prefLabel>
        <label:plural xml:lang="en">Data Containers</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#Dead -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#Dead">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#Casualty"/>
        <owl:disjointWith rdf:resource="http://hxl.humanitarianresponse.info/ns/#Injured"/>
        <owl:disjointWith rdf:resource="http://hxl.humanitarianresponse.info/ns/#Missing"/>
        <rdfs:comment>A (reported) death.&lt;br /&gt;&lt;small&gt;Term origin: ACAPS DSS, HPM Ethiopia, Shelter Cluster, Humanitarian Profile (&lt;em&gt;Dead&lt;/em&gt;).&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:altLabel xml:lang="en">Dead</skos:altLabel>
        <skos:prefLabel xml:lang="en">Deaths</skos:prefLabel>
        <label:plural xml:lang="en">Deaths</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#Displaced -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#Displaced">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#AffectedPopulation"/>
        <owl:disjointWith rdf:resource="http://hxl.humanitarianresponse.info/ns/#NonDisplaced"/>
        <rdfs:comment>Refers to internally displaced persons, refugees, and others of concern.&lt;br /&gt;&lt;small&gt;Term origin: Humanitarian Profile, ACAPS DSS (&lt;em&gt;Displacement&lt;/em&gt;)&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:prefLabel xml:lang="en">Displaced Population</skos:prefLabel>
        <label:plural xml:lang="en">Displaced Populations</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#DisplacedLocationType -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#DisplacedLocationType">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <rdfs:comment xml:lang="en">Generic class for types of displaced locations; instances should be created for actual displaced location types and must have identifiers that do not start with a number. Examples: &lt;em&gt;self-settled camp&lt;/em&gt; or &lt;em&gt;collective centre&lt;/em&gt;. Each DisplacedLocationType should be added to at least one &lt;a href=&apos;#DisplacedLocationTypeSet&apos;&gt;DisplacedLocationTypeSet&lt;/a&gt; following the conventions of &lt;a href=&apos;http://www.w3.org/TR/rdf-schema/#ch_member&apos;&gt;rdfs:member&lt;/a&gt;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#geolocationSection"/>
        <skos:prefLabel xml:lang="en">Displaced Location Type</skos:prefLabel>
        <label:plural xml:lang="en">Displaced Location Types</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#DisplacedLocationTypeSet -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#DisplacedLocationTypeSet">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Bag"/>
        <rdfs:comment xml:lang="en">Generic class for classes describing the housing situation in which a displaced population is living. Subclasses of this class represent a coherent set of these location types which may vary from emergency to emergency.  The individual types are instances of the subclass. &lt;a href=&apos;#DisplacedLocationType&apos;&gt;DisplacedLocationTypes&lt;/a&gt; should be added to a DisplacedLocationTypeSet following the conventions of &lt;a href=&apos;http://www.w3.org/TR/rdf-schema/#ch_member&apos;&gt;rdfs:member&lt;/a&gt;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#geolocationSection"/>
        <skos:prefLabel xml:lang="en">Displaced Persons Location Type Set</skos:prefLabel>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#Distribution -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#Distribution">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#Response"/>
        <hxl:topLevelConcept rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</hxl:topLevelConcept>
        <rdfs:comment xml:lang="en">A distribution as part of response activities, e.g., a distribution of food or shelter/household kits.&lt;br /&gt;&lt;small&gt;This class has been added for demo purposes only, a proper definition will be provided in subsequent versions of the vocabulary.&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#ResponseSection"/>
        <skos:prefLabel xml:lang="en">Distribution</skos:prefLabel>
        <label:plural xml:lang="en">Distributions</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#Emergency -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#Emergency">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#Situation"/>
        <rdfs:comment>&lt;p&gt;Any situation in which life or well-being will be threatened  unless immediate  and  appropriate   action  is  taken,  and  which demands   an   extraordinary   response   and   exceptional measures (from UNHCR Handbook for Emergencies).&lt;/p&gt;&lt;!-- &lt;p&gt;A sudden crisis situation or event requiring immediate action (WART) &lt;/p&gt;&lt;p&gt;Any situation in which there is an exceptional and widespread threat to life, health or basic  subsistence,  that  is  beyond  the  coping  capacity  of individuals and the community  (Oxfam GB Emergency Response Manual). &lt;/p&gt;&lt;p&gt;Where and when a disaster provokes an immediate, exceptional and widespread threat to life, health or basic subsistence, which overwhelms the coping capacity of individual and community, implying need of external assistance (source: ACFIN Emergency module)&lt;/p&gt; --&gt;&lt;small&gt;Term origin: CAP OPS, Pakistan SRF, ACAPS DSS, CERF, MIRA.&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#situationSection"/>
        <skos:prefLabel xml:lang="en">Emergency</skos:prefLabel>
        <label:plural xml:lang="en">Emergencies</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#HXLer -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#HXLer">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#Source"/>
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">Superclass for any individuals or organizations interacting with HXL.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#metadataSection"/>
        <skos:prefLabel xml:lang="en">HXLer</skos:prefLabel>
        <label:plural xml:lang="en">HXLers</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#HostPopulation -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#HostPopulation">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#NonDisplaced"/>
        <owl:disjointWith rdf:resource="http://hxl.humanitarianresponse.info/ns/#NonHostPopulation"/>
        <rdfs:comment>Persons who are part of a host community or family receiving affected people. Due to the stress placed on the host families and communities, they are considered part of the humanitarian caseload.&lt;br /&gt;&lt;small&gt;Term origin: Humanitarian Profile.&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:prefLabel xml:lang="en">Host Population</skos:prefLabel>
        <label:plural xml:lang="en">Host Populations</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#IDP -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#IDP">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#Displaced"/>
        <owl:disjointWith rdf:resource="http://hxl.humanitarianresponse.info/ns/#Others"/>
        <owl:disjointWith rdf:resource="http://hxl.humanitarianresponse.info/ns/#RefugeesAsylumSeekers"/>
        <rdfs:comment>A group of internally displaced persons (IDPs).&lt;br /&gt;&lt;small&gt;Term origin: Humanitarian Profile, CERF, HPM Ethiopia&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:altLabel xml:lang="en">IDP</skos:altLabel>
        <skos:prefLabel xml:lang="en">Internally Displaced Population</skos:prefLabel>
        <label:plural xml:lang="en">Internally Displaced Populations</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#Incident -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#Incident">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#LocatedBaseClass"/>
        <hxl:topLevelConcept rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</hxl:topLevelConcept>
        <rdfs:comment xml:lang="en">Generic class to cover all types of incidents that lead to a situation where parts of the local population are affected in a way that requires intervention. Examples include natural disaster, outbreak of an armed conflict, outbreak of a disease in an APL, etc.&lt;br /&gt;&lt;small&gt;This class has been added for demo purposes only, a proper definition will be provided in subsequent versions of the vocabulary.&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#situationSection"/>
        <skos:prefLabel xml:lang="en">Incident</skos:prefLabel>
        <label:plural xml:lang="en">Incidents</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#Injured -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#Injured">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#Casualty"/>
        <rdfs:comment>Indicates the Injured population.&lt;br /&gt;&lt;small&gt;Term origin: ACAPS DSS, Humanitarian Profile, Shelter Cluster, HERAMS Checklist (&lt;em&gt;Injury&lt;/em&gt;)&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:prefLabel xml:lang="en">Injured</skos:prefLabel>
        <label:plural xml:lang="en">Injured</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#LocatedBaseClass -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#LocatedBaseClass">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <rdfs:comment xml:lang="en">Abstract base class for things that have a location. Any class in HXL that represents located things is a subclass of hxl:BaseClass.</rdfs:comment>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#Missing -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#Missing">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#Casualty"/>
        <rdfs:comment>Associated with &lt;i&gt;Missing Persons&lt;/i&gt; as a major humanitarian concern in complex emergencies where many people go missing, causing anguish and uncertainty for their families and friends. See &lt;em&gt; ICRC &lt;/em&gt;. &lt;br /&gt;&lt;small&gt;Term origin: Humanitarian Profile.&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:prefLabel xml:lang="en">Missing</skos:prefLabel>
        <label:plural xml:lang="en">Missing</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#Need -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#Need">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#BaseClass"/>
        <hxl:topLevelConcept rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</hxl:topLevelConcept>
        <rdfs:comment xml:lang="en">Needs of an affected population, such as water, shelter, and sanitation.&lt;br /&gt;&lt;small&gt;This class has been added for demo purposes only, a proper definition will be provided in subsequent versions of the vocabulary.&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#situationSection"/>
        <skos:prefLabel xml:lang="en">Need</skos:prefLabel>
        <label:plural xml:lang="en">Needs</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#NonDisplaced -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#NonDisplaced">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#AffectedPopulation"/>
        <rdfs:comment>Refers to the part of the affected population that is not displaced (i.e., host or non-host);.&lt;br /&gt;&lt;small&gt;Term origin: Humanitarian Profile.&lt;/small&gt;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://hxl.humanitarianresponse.info/ns/#humanitarianProfileSection"/>
        <skos:prefLabel xml:lang="en">Non-Displaced Population</skos:prefLabel>
        <label:plural xml:lang="en">Non-Displaced Populations</label:plural>
    </owl:Class>
    


    <!-- http://hxl.humanitarianresponse.info/ns/#NonHostPopulation -->

    <owl:Class rdf:about="http://hxl.humanitarianresponse.info/ns/#NonHostPopulation">
        <rdfs:subClassOf rdf:resource="http://hxl.humanitarianresponse.info/ns/#NonDisplaced"/>
