    <rdf:Description rdf:about="http://observedchange.com/moac/ns#">
        <dcterm:description xml:lang="en">MOAC, the Management of a Crisis Vocabulary, is a lightweight vocabulary aiming to provide terms to enable practitioners to relate different &quot;things&quot; in crisis management activities together as Linked Data. The initial MOAC terms originated from the Inter Agency Standing Committee (IASC), Emergency Shelter Cluster in Haiti, UNOCHA 3W Who What Where Contact Database and</dcterm:description>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#NFINeededAt">
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
        <rdfs:comment xml:lang="en">NFINeededAt relates &quot;where&quot; Non Food Items response might be necessary.</rdfs:comment>
        <rdfs:label xml:lang="en">NFINeededAt</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#affectedby">
        <rdfs:comment xml:lang="en">affectedby indicates the relation of people being affected by certain disaster.</rdfs:comment>
        <rdfs:comment xml:lang="en">affectedby indicates disaster which has influence on population.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
        <rdfs:label xml:lang="en">affectedby</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#approved">
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
        <rdfs:comment xml:lang="en">approved is a data property to attest a event or incident or document is true or not.</rdfs:comment>
        <rdfs:label xml:lang="en">approved</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#atGeoLocationLatitude">
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
        <rdfs:comment xml:lang="en">atGeoLocationLatitude is a data property used to link location or &quot;Where&quot; with specific latitude in the form of decimal degree.</rdfs:comment>
        <rdfs:label xml:lang="en">atLocation</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#atGeoLocationLongitude">
        <rdfs:label xml:lang="en">atLocation</rdfs:label>
        <rdfs:comment xml:lang="en">GeoLocationLatitude is a data property used to link location or &quot;Where&quot; with specific longitude in the form of decimal degree.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#atLocation">
        <rdfs:label xml:lang="en">atLocation</rdfs:label>
        <rdfs:comment xml:lang="en">atLocation is a data property to attest agency to a certain location.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#belongsToCluster">
        <rdfs:label xml:lang="en">belongsToCluster</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
        <rdfs:comment xml:lang="en">belongsToCluster indicates the organisation which belongs to a specific IASC cluster for example, emergency Shelter cluster or Food Cluster.</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#category">
        <rdfs:label xml:lang="en">title</rdfs:label>
        <rdfs:comment xml:lang="en">category is about the sort of an incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#clusterActivity">
        <rdfs:label xml:lang="en">atLocation</rdfs:label>
        <rdfs:comment xml:lang="en">clusterActivity is a property to relate humanitarian Clusters with ongoing humanitarian response to meet the needs of affected population.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#countInPipeline">
        <rdfs:comment xml:lang="en">countInPipeline indicates the number of relief items that are &quot;in the process&quot; of arriving from a different location.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
        <rdfs:label xml:lang="en">countInPipeline</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#countInStock">
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
        <rdfs:label xml:lang="en">countInStock</rdfs:label>
        <rdfs:comment xml:lang="en">countInStock indicates the number of items currently stockpiled either at a store or at a distribution location.</rdfs:comment>
    </rdf:Description>
    <rdf:Description rdf:about="http://observedchange.com/moac/ns#countOfAffectedPopulation">
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
        <rdfs:comment xml:lang="en">countOfAffectedPopulation indicates the total number of people affected by a certain disaster.</rdfs:comment>
