    <!-- http://observedchange.com/moac/ns#AffectedPopulation -->

    <owl:Class rdf:about="http://observedchange.com/moac/ns#AffectedPopulation">
        <rdfs:comment xml:lang="en">AffectedPopulation is a group of disaster affecgted people in need of humanitarian support.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
        <rdfs:label xml:lang="en">AffectedPopulation</rdfs:label>
    </owl:Class>
    


    <!-- http://observedchange.com/moac/ns#Agency -->

    <owl:Class rdf:about="http://observedchange.com/moac/ns#Agency">
        <rdfs:comment xml:lang="en">Agency is an organization that aims to manage a crisis.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://observedchange.com/moac/ns"/>
        <rdfs:label xml:lang="en">Agency</rdfs:label>
    </owl:Class>
    


